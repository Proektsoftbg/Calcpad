                          Header="{x:Static wpf:MainWindowResources.Save}"/>
                <MenuItem Command="ApplicationCommands.SaveAs" 
                          Header="{x:Static wpf:MainWindowResources.SaveAs}"/>
                <MenuItem Click="MenuCli_Click" 
                          Header="{x:Static wpf:MainWindowResources.CommandLineInterpreter}"/>
                <MenuItem Command="ApplicationCommands.Close" 
                          Header="{x:Static wpf:MainWindowResources.Exit}"/>
            </MenuItem>
