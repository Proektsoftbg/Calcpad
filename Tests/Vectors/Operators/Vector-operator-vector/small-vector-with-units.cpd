f(a; b) = a ≡ b','c = f(a; b)

a0 = [0cm; 0cm; 0cm; 0cm; 0cm; 0m]
b0 = [0cm; 0cm; 0cm; 0cm; 0cm; 0m]
c0 = a0 ≡ b0
f(a0; b0)

a1 = [0.5cm; 6.5m; 0m; -7dm; 4.6m]
b1 = [0.5cm; -2.9m; 0m; 7.3dm]
c1 = a1 ≡ b1
f(a1; b1)

"Not equal to
a = [-4.3mm; 0mm; 6.9m; -2.8mm; 3mm]
b = [8.2cm; -5cm; 0cm; 2.7cm; -1.4cm]
