f(a0; b0)

a1 = [-3mm; 6.5mm; 0mm; -7mm; 4.6mm]
b1 = [0.5cm; -2.9cm; 0cm; 7.3cm]
c1 = a1 ≠ b1
f(a1; b1)

" Less than
a = [0m; -6.7m; 3.8m; -2m; 9.5m]
