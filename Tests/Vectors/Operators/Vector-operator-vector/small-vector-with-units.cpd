a1 = [6m; -2.5m; 0m; 4.3m; -8.1m]
b1 = [8.6dm; 0dm; -5.2dm; 1.9dm]
c1 = a1 < b1
c1 = f(a1; b1)

"Greater than
a = [4.7dm; -8dm; 0dm; 2.3dm; -6.5dm]
b = [-3m; 6.2m; 0m; -5.1m; 7.4m]
