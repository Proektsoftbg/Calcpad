'<h1>Arithmetic operators</h1>
"Negation
a = [-3m; 0cm; 4.5mm; -7dm; 2.25m]
c = -a
f(a) = -a','c = f(a)

a0 = [0cm; 0cm; 0dm; 0m; 0mm; 0dm]
c0 = -a0
c0 = f(a0)

a1 = [1.1m; -2.2cm; 0dm; 3.3mm; -4.4m]
c1 = -a1
c1 = f(a1)

"Addition
a = [5dm; 12.3dm; -44dm; 13dm; 21.4dm]
b = [-5mm; 12.5mm; 10mm; 0mm; -21.3mm]
c = a + b
f(a; b) = a + b','c = f(a; b)

a0 = [0dm; 0dm; 0dm; 0dm; 0dm; 0dm]
b0 = [0cm; 0cm; 0cm; 0cm; 0cm; 0cm]
c0 = a0 + b0
c0 = f(a0; b0)

a1 = [23.1mm; 12mm; -37mm; 5mm; 48.7mm]
b1 = [-19m; 41m; 3m; 31.2m]
c1 = a1 + b1
c1 = f(a1; b1)

"Subtraction
a = [-3m; 0m; 4.5m; -7m; 2.25m]
b = [-2.5dm; 0dm; 3.75dm; -1dm; 4.2dm]
c = a - b
f(a; b) = a - b','c = f(a; b)

a0 = [0m; 0m; 0m; 0m; 0m; 0m]
b0 = [0dm; 0dm; 0dm; 0dm; 0dm; 0dm]
c0 = a0 - b0
c0 = f(a0; b0)

a1 = [1.1m; -2.2m; 0m; 3.3m; -4.4m]
b1 = [0.5dm; 0.5dm; -1.5dm; 1.5dm]
c1 = a1 - b1
c1 = f(a1; b1)

"Multiplication
a = [0.1kg; -0.2cm; 0.3mm; -0.4kg; 0.5cm]
b = [-1.5t; -1m; 0cm; 1t; 1.5m]
c = a*b
f(a; b) = a*b','c = f(a; b)

a0 = [0kg; 0cm; 0mm; 0kg; 0cm; 0mm]
b0 = [0t; 0m; 0cm; 0t; 0m; 0cm]
c0 = a0*b0
c0 = f(a0; b0)

a1 = [0.25kg; -0.5cm; 0.75mm; -1kg; 1.25cm]
b1 = [-2.75t; -2.5m; -2.25cm; -2t]
c1 = a1*b1
c1 = f(a1; b1)

"Division
a = [0.1kN; -0.2kN; 0.3kN; -0.4kN; 0.5kN]
b = [-1.5m; -1m; 0m; 1m; 1.5m]
c = a/b
f(a; b) = a/b','c = f(a; b)

a0 = [0kN; 0kN; 0kN; 0kN; 0kN; 0kN]
b0 = [0m; 0m; 0m; 0m; 0m; 0m]
c0 = a0/b0
c0 = f(a0; b0)

a1 = [0.25kN; -0.5kN; 0.75kN; -1kN; 27.25kN]
b1 = [-2.75m; -2.5m; -2.25m; -2m]
c1 = a1/b1
c1 = f(a1; b1)

"Exponentiation
a = [-4.25m; 0m; 7.89m; -10m; 3.14159m]
b = [3.7; -5.2; 0; 8; -2.6]
c = a^b
f(a; b) = a^b','c = f(a; b)

a0 = [0dm; 0dm; 0dm; 0dm; 0dm; 0dm]
b0 = [0; 0; 0; 0; 0; 0]
c0 = a0^b0
c0 = f(a0; b0)

a1 = [-1m; 4.8m; 0.2m; -6m; 10m]
b1 = [7.5; -3; 0; 9.6]
c1 = a1^b1
c1 = f(a1; b1)

"Division bar
a = [0.1dm; -0.2dm; 0.3dm; -0.4dm; 0.5dm]
b = [-1.5m; -1m; 0m; 1m; 1.5m]
c = a÷b
f(a; b) = a÷b','c = f(a; b)

a0 = [0m; 0m; 0m; 0m; 0m; 0m]
b0 = Vector(5)
c0 = a0÷b0
c0 = f(a0; b0)

a1 = [-5cm; 8.4cm; 0cm; -2.1cm; 7cm]
b1 = [2; -9.2; 0; 6.7]
c1 = a1÷b1
c1 = f(a1; b1)

"Integer division
a = [-0.5m; 3m; 0m; -7.8m; 5.6m]
b = [4.2dm; -6dm; 0dm; 1.8dm; -3.1dm]
c = a\b
f(a; b) = a\b','c = f(a; b)

a0 = [0m; 0m; 0m; 0m; 0m; 0m]
b0
c0 = a0\b0
c0 = f(a0; b0)

a1 = [0; 7; -2.3; 5.1; -8.6]
b1 = [-3cm; 0cm; 9.6cm; -4.3cm]
c1 = a1\b1
c1 = f(a1; b1)

"Modulo (remainder)
a = [-4.3m; 0m; 6.9m; -2.8m; 3m]
b = [8.2; -5; 0; 2.7; -1.4]
c = a⦼b
f(a; b) = a⦼b','c = f(a; b)

a0 = [0dm; 0dm; 0dm; 0dm; 0dm; 0dm]
b0
c0 = a0⦼b0
c0 = f(a0; b0)

a1 = [-3dm; 6.5dm; 0dm; -7dm; 4.6]
b1 = [0.5; -2.9; 0; 7.3]
c1 = a1⦼b1
c1 = f(a1; b1)

"Factorial
a = [4; 2; 6; 2; 3]
c = a!
f(a) = a!','c = f(a)

a0 = Vector(5)
c0 = a0!
c0 = f(a0)

a2 = [5; 2; 0; 7; 6]
c2 = a2!

'<h1>Relational operators</h1>
"Equal to
a = [8.2m; 0m; 6.9m; -2.8m; 3m]
b = [8.2cm; -5cm; 0cm; 2.7cm; -1.4cm]
